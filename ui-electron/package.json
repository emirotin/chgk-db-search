{
  "name": "renderer",
  "version": "0.1.0",
  "private": true,
  "main": "start-electron.js",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "start":
      "concurrently 'BROWSER=none npm run start-react' 'wait-on http://localhost:3000/ && IS_DEV=1 npm run electron'"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "electron": "^1.7.9",
    "wait-on": "^2.0.2"
  },
  "homepage": "./"
}
